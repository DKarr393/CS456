{
    "manifest_version": 2,
    "name": "ColorCorrectionExt",
    "version": "1.0.0",
    "description": "An extension designed to color correct sites for colorblind users.",
    "icons": {
        "16": "",
        "48": "",
        "128": ""
    },
    "page_action": {
        "default_icon": {
            "16": "",
            "48": "",
            "128": ""
        }
    },
    "content_scripts": [{
        "matches": ["https//www.google.com/*"],
        "css": ["main.css"]
    }]
  }